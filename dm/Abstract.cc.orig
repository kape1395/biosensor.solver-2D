/* **********   Model   ***************************************************** */
/* ************************************************************************** */
#ifdef DM_Abstract_TT



template<class SA, int x, int y>
Model<SA, x, y>::Model(
    Factory<SA> *factory,
    Dimension   *dimH[x],
    Dimension   *dimV[y]
)
{
    this->factory = factory;
    this->dimH = dimH;
    this->dimV = dimV;

    for (int i = 0; i < x; i++)
    {
        for (int j = 0; j < y; j++)
        {
            area[i][j] = factory->newArea(dimH[i], dimV[j]);
        }
    }

    for (i = 0; i < x; i++)
    {
        for (j = 0; j <= y; j++)
        {
            boundH[i][j] = factory->newBound(
                dimH[j],
                j == 0 ? 0 : area[i][j-1],
                i == y ? 0 : area[i][j]
            );
        }
    }
}



template<class SA, int x, int y>
Model<SA, x, y>::~Model()
{

}



#endif
